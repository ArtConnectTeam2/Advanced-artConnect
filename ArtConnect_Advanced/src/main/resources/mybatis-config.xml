<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- SQL문 정의할 때 VO표현을 간단하게 하고 싶은 경우. 별명을 지어서 사용 -->
	<typeAliases>
		<!-- 양민주 -->
		<typeAlias type="com.multi.artConnect.member.MemberVO" alias="memberVO"></typeAlias>
		<typeAlias type="com.multi.artConnect.member.AuthVO" alias="authVO"></typeAlias>
		<!-- 장태훈 -->
		<typeAlias type="com.multi.artConnect.reservation.ProgramVO" alias="programVO"></typeAlias>
		<typeAlias type="com.multi.artConnect.reservation.ReservationVO" alias="reservationVO"></typeAlias>
		<typeAlias type="com.multi.artConnect.reservation.GalleryVO_reserve" alias="galleryVO_reserve"></typeAlias>
		<!-- 이서영 -->
		<typeAlias type="com.multi.artConnect.review.BoardVO" alias="boardVO"></typeAlias>
		<typeAlias type="com.multi.artConnect.review.ReplyVO" alias="replyVO"></typeAlias>
		<!-- 최영태 -->
		<typeAlias type="com.multi.artConnect.gallery.GalleryVO" alias="galleryVO"></typeAlias>
		<typeAlias type="com.multi.artConnect.gallery.ProgramVO_gallery" alias="programVO_gallery"></typeAlias>
		<!-- 강동일 -->
		<typeAlias type="com.multi.artConnect.notice.NoticeVO" alias="noticeVO"></typeAlias>
		<typeAlias type="com.multi.artConnect.notice.QnaVO" alias="qnaVO"></typeAlias>
		<typeAlias type="com.multi.artConnect.notice.CommentVO" alias="CommentVO"></typeAlias>
	</typeAliases>

</configuration>